<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tatacruciblehackathon">
    <uses-permission android:name="android.permission.CAMERA">

    </uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_qr_code_scanner_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TataCrucibleHackathon">
        <activity android:name=".Scanner"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".products"></activity>
        <activity android:name=".PreviousScan"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>